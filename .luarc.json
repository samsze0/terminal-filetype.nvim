{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "runtime.version": "LuaJIT",
  "workspace": {
    "library": [
      "lua",
      "$VIMRUNTIME/lua",
      "${3rd}/luv/library",
      "../utils.nvim/lua",
      ".test/lua"
    ],
    "checkThirdParty": false
  },
  "diagnostics": {
    "groupseverity": {
      "await": "warning",
      "strong": "warning"
    }
  }
}
